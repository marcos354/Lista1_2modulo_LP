def maiorMenor():
  maior = 0
  menor = 0
  for contador in range (5):
    num = int(input("Digite um valor: "))
    if contador == 0:
      maior = menor = num
    else:
       if num > maior:
         maior = num
       if num < menor:
         menor = num 
  print("Maior número encontrado: ", maior)
  print("Menor número encontrado: ", menor)
def main():
  maiorMenor()
main()
